{
  "name": "bb26-spreadsheet",
  "version": "1.5.2",
  "description": "Bijective base-26 number system utilities for spreadsheet software",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/istareatscreens/bb26-spreadsheet.git"
  },
  "bugs": {
    "url": "https://github.com/istareatscreens/bb26-spreadsheet/issues"
  },
  "types": "lib",
  "files": [
    "lib"
  ],
  "keywords": [
    "bb26",
    "base-26",
    "spreadsheet",
    "excel",
    "bijective base-26",
    "Hexavigesimal"
  ],
  "scripts": {
    "build": "tsc -p .",
    "lint": "eslint src/*.ts",
    "test": "nyc --reporter=html --reporter=text mocha",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov"
  },
  "nyc": {
    "include": [
      "src"
    ],
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "reporter": [
      "html"
    ],
    "all": true
  },
  "sideEffects": false,
  "author": "Stephen Charbonneau",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "4.2.14",
    "@types/mocha": "7.0.2",
    "@typescript-eslint/eslint-plugin": "2.33.0",
    "@typescript-eslint/parser": "2.33.0",
    "chai": "4.2.0",
    "codecov": "3.8.1",
    "eslint": "6.8.0",
    "istanbul": "0.4.5",
    "mocha": "7.1.2",
    "mocha-lcov-reporter": "1.3.0",
    "nyc": "15.1.0",
    "prettier": "2.1.2",
    "source-map-support": "0.5.19",
    "ts-node": "8.10.2",
    "typescript": "3.9.7"
  }
}
